Cufon.replace("nav.cufon1 a",{fontFamily:"Order Rg",hover:true});Cufon.replace("h1, h3, .cufon2",{fontFamily:"Placard Condensed"});Cufon.now();$(document).ready(function(){replace_email();lightbox_handler();code_helper()});if(top.location!=self.location){top.location.replace(self.location.href)}function replace_email(){var a="corvidworks.com";var b="span.email";$(b).each(function(){var d=$(this).attr("title")+"@"+a;var c=$("<a>").attr("href","mailto:"+d);$(c).append(d);$(this).replaceWith(c)});$("span.email_me").show()}function tooltip_helper(){$("a.arrow").hover(function(){$(this).removeAttr("title");$(this).parent().next("div.tooltip").show()},function(){$(this).parent().next("div.tooltip").hide()})}function scroll_helper(){var a="a.scroller";$(a).click(function(b){var d=$(this.hash);d=d.length&&d||$("[name="+this.hash.slice(1)+"]");if(d.length){var c=d.offset().top;$("html,body").animate({scrollTop:c},800)}b.preventDefault()})}function lightbox_handler(){$("a.screenshot").colorbox({maxWidth:"95%",maxHeight:"100%",scalePhotos:true});$("a.lightbox_content_link").each(function(){var a="#"+this.hash.slice(1);$(this).colorbox({width:"60%",inline:true,href:a})})}function contact_helper(){var a="/contact/";var c="#contact";var b="#results";$(b+" a").click(function(d){if($(b).hasClass("error")){jQuery.get(a+"clear");$(c+" input").removeClass("error");$(c+" textarea").removeClass("error")}$(b).hide();d.preventDefault()})}function code_helper(){$("pre").addClass("prettyprint");prettyPrint()};