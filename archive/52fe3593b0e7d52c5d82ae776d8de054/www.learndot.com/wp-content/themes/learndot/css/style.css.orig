/* CSS for Learndot by Awesome JS */

body{
	font-family:"Apercu Light", Calibri, sans-serif; font-size-adjust:0.508; font-weight:normal; font-style:normal; 
	overflow-x:hidden;
	position:relative;	
}

textarea,input{
	font-family: 'Apercu Regular', Helvetica, sans-serif;
}

a, a:focus{
	outline:0;
	color:#5BBCD5;
}

a:hover{
	color:#2189A4;
	text-decoration:underline;
}

.navbar{
	margin:0;
	position:absolute;
	top:0px; left:0px;
	width:100%;
	z-index:20;
}

.navbar-inverse .navbar-inner{
	background:#bb5435;
	background:rgba(35,35,35,0.2);
	padding:2px 0;
	border:none;
	-webkit-border-radius:0px;
	-moz-border-radius:0px;
	border-radius:0px;
	filter:none!important;
}

.tooltip{
	filter: alpha(opacity=100)!important;
}

/* header */

.navbar .phone-icon{
	padding:10px 15px 10px 35px;
	margin:0;
	color:#e0a28f;
	font-size:14px;
	line-height:16px;
	background:url(../img/icons.png) no-repeat -20px -5px;
}

	.single-post .phone-icon{
		color:#fff!important;
	}

	.navbar .phone-icon strong{
		font-size:16px;
		color:#fff;
		font-weight:normal;
		font-family: 'Apercu Bold', Helvetica, sans-serif;
	}

	.nav > li > a{
		font-size:16px;
		color:#fff!important;
		font-weight:normal;
		font-family: 'Apercu Bold', Helvetica, sans-serif;
		text-transform:uppercase;
	}

	.nav > li > a:hover{
		text-decoration:underline!important;
	}

/* featured area */

#featured-area{
	padding:87px 0 75px 0;
	height:608px;
}	

#featured-area .container{
	position:relative;
}

#featured-area img{
	width:95%;
	position:absolute;
	top:53px; left:0px;
	z-index:1;
}

	#logo{
		margin:0; padding:0;
		position:relative;
		z-index:10;
	}

		#logo a{
			text-indent:-9999em;
			background:url(../img/logo.png) no-repeat top center;
			width:353px; height:61px;
			display:block;
			margin:0 auto;
		}
	
#featured-area h2{
	font-size:90px;
	line-height:90px;
	padding:220px 0 0 0;
	color:#fff;
	margin:0;
	font-weight:normal;
	font-family: 'minion-regular', 'Times New Roman', Times, serif;
	position:relative;
	z-index:10;
}

/* circles */

#circles{
	padding:26px 0;
}

	#circles .span4{
		padding-top:40px;
	}

#circles h3{
	font-size:32px;
	line-height:32px;
	color:#393b3e;
	font-family: 'Apercu Bold', Helvetica, sans-serif;
	font-weight:normal;
}

#circles p{
	font-size:18px;
	color:#96989b;
	font-family: 'Apercu Bold', Helvetica, sans-serif;
}

ul.trials{
	margin:0; padding:0;
	float:right;	
	position:relative;
}

	ul.trials::after{
		content:'';
		position:absolute;
		left:-70px;
		top:50%;
		margin-top:-22px;
		width:45px; height:45px;
		display:block;
		background:url(../img/icons.png) no-repeat -14px -357px;
	}

	ul.trials li{
		float:left; display:block;
		margin-right:16px;
		list-style:none;
	}

		ul.trials li a{
			width:174px;
			height:174px;
			line-height:174px;
			display:block;
			text-align:center;
			font-size:21px;
			font-family: 'Apercu Bold', Helvetica, sans-serif;
			color:#fff;
			text-transform:uppercase;
			background:#e1603a;
			-webkit-border-radius:90px;
			-moz-border-radius:90px;
			border-radius:90px;
			-webkit-backface-visibility:hidden;
			-webkit-transition: box-shadow linear 0.3s;
			-moz-transition:box-shadow linear 0.3s;
			-o-transition: box-shadow linear 0.3s;
			transition: box-shadow linear 0.3s;			
		}
	
			ul.trials li a:hover{
				text-decoration:none;
			}

			ul.trials li.black a{
				background:#393b3e;
			}

			ul.trials li.skyblue a{
				background:#5bbcd5;
			}

			ul.trials li.orange a:hover{
				-webkit-box-shadow:0 0 15px #e1603a;
				-moz-box-shadow:0 0 15px #e1603a;
				box-shadow:0 0 15px #e1603a;
			}

			ul.trials li.black a:hover{
				-webkit-box-shadow:0 0 15px #393b3e;
				-moz-box-shadow:0 0 15px #393b3e;
				box-shadow:0 0 15px #393b3e;
			}

			ul.trials li.skyblue a:hover{
				-webkit-box-shadow:0 0 15px #5bbcd5;
				-moz-box-shadow:0 0 15px #5bbcd5;
				box-shadow:0 0 15px #5bbcd5;
			}

.separator{
	border-top:1px solid #d9dee1;
}

/* cta */

#cta{
	padding:50px 0 94px 0;
}

	#cta p{
		font-size:32px;
		line-height:32px;
		color:#6d6f72;
		padding:0 150px 0 0;
	}

	#cta p a{
		color:#5bbcd5;
		font-family: 'Apercu Bold', Helvetica, sans-serif;
		text-decoration:underline;
	}

	#cta p a:hover{
		color:#2189A4;
	}

/* university */

#university{
	padding:60px 0;
}

	#university .container{
		position:relative;
	}

#university h2{
	font-size:60px;
	font-weight:normal;
	font-family: 'minion-regular', 'Times New Roman', Times, serif;
	text-align:left;
	color:#fff;
}

#university img{
	position:absolute;
	right:-86px;
	top:-122px;
	z-index:20;
}

/* tablet-box */

#tablet-box{
	padding:50px 0 50px 0;
}

	.tablet-title{
		font-size:28px;
		color:#6d6f72;
		line-height:60px;		
	}

	.hightlight{
		line-height:50px;
		padding:10px;
		background:#7cc9dd;
		color:#fff;
		-webkit-border-radius:5px;
		-moz-border-radius:5px;
		border-radius:5px;
	}

	.tablet-tooltips{
		padding:88px 0 50px 0;
		position:relative;
	}

		.tablet-tooltips .tooltip{
			opacity:1;
			background:#696d71;
			color:#fff;
			display:block;
			padding:15px 20px 15px 15px;
			font-size:18px;
			font-family: 'Apercu Bold', Helvetica, sans-serif;
			-webkit-border-radius:8px;
			-moz-border-radius:8px;
			border-radius:8px;
		}

			.tablet-tooltips .tooltip:hover{
				text-decoration:none;
			}

			.tablet-tooltips .tooltip.right::after{
				content:'';
				position:absolute;
				border-top: 16px solid transparent;
				border-bottom: 16px solid transparent; 				
				border-right:16px solid #696d71; 
				left:-8px; top:50%;
				margin-top:-15px;		
				width:0px; height:0px;
			}

			.tablet-tooltips .tooltip.left::after{
				content:'';
				position:absolute;
				right:-8px; top:50%;
				margin-top:-15px;	
				border-top: 16px solid transparent;
				border-bottom: 16px solid transparent; 				
				border-left:12px solid #696d71; 
				width:0px; height:0px;
			}

		#tooltip-1{
			width:182px;
			left:12%;
			top:388px;
		}

		#tooltip-2{
			width:182px;
			right:-1%;
			top:274px;
		}

		#tooltip-3{
			width:182px;
			right:-4%;
			top:508px;
		}

	#tablet-box img{
		width:100%;
		margin-left:0px;
	}

	.arrow-bottom{
		width:52px; height:50px;
		display:block;
		position:absolute;
		top:30px; left:46%; margin-left:-25px;
		background:url(../img/icons.png) no-repeat -9px -297px;
	}

/* features */

#features{
	padding:60px 0 130px 0;
	border-top:1px solid #d8dbde;
	box-shadow:inset 0 5px 5px -3px rgba(0,0,0,0.1);
	background:#f3f5f7;
}

	#main-features{
		margin:0; padding:0;
	}

		#main-features li{
			float:left; display:block;
			text-align:center;
		}

		#main-features li strong{
			width:100%;
			display:block;
			font-size:27px;
			line-height:27px;
			color:#727272;
			font-weight:normal;
			font-family: 'Apercu Bold', Helvetica, sans-serif;
			padding:20px 0 6px;
		}

		#main-features li p{
			width:100%;
			display:block;
			color:#727272;
			font-size:15px;
			margin:0;
		}

	#list-features{
		background:#fff;
		border:1px solid #dbdcdf;
		-webkit-border-radius:10px;
		-moz-border-radius:10px;
		border-radius:10px;
		margin:80px 0 0 0;
	}

		.feature{
			border-bottom:1px solid #dbdcdf;
			padding:60px 76px 56px 262px;
			position:relative;
		}

			.feature::after{
				content:'';
				position:absolute;
				border-top: 18px solid #fff;
				border-right: 18px solid transparent; 				
				border-left:18px solid transparent; 
				left:50%; bottom:-18px;
				margin-left:-19px;		
			}

			.feature::before{
				content:'';
				position:absolute;
				border-top: 19px solid #dbdcdf;
				border-right: 19px solid transparent; 				
				border-left:19px solid transparent; 
				left:50%; bottom:-19px;
				margin-left:-20px;	
			}

			.feature.last{
				border-bottom:none;
			}

				.feature.last::after{
					display:none;
				}

				.feature.last::before{
					display:none;
				}

			.feature img{
				position:absolute;
				top:50px; left:60px;
			}

			.feature h3{
				font-size:32px;
				color:#6d6f72;
				font-family: 'Apercu Bold', Helvetica, sans-serif;
				font-weight:normal;
			}

			.feature p{
				font-size:21px;
				line-height:24px;
				color:#6d6f72;
			}

			.feature p a, .feature a{
				color:#5bbcd5;
				text-decoration:underline;
			}

				.feature a:hover{
					color:#2189A4;
				}

/* possibilities headline */

#possibilities{
	padding:60px 0;
}

	#possibilities .container{
		position:relative;
	}

#possibilities h2{
	font-size:60px;
	font-weight:normal;
	font-family: 'minion-regular', 'Times New Roman', Times, serif;
	text-align:left;
	color:#fff;
}

#possibilities img{
	position:absolute;
	right: -176px;
	top: -162px;
	z-index:20;
}

/* timeline */

#timeline{
	padding:90px 0 50px;
}

ul.timeline-events{
	margin:0 auto; padding:0;
	width:7px;
	height:760px;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;
	background:#848689 url(../img/timeline-bg.jpg) no-repeat bottom center;
	position:relative;
}

ul.timeline-events li{
	background:#ffffff;
	width:20px; height:20px;
	-webkit-border-radius:20px;
	-moz-border-radius:20px;
	border-radius:20px;
	border:5px solid #848689;
	list-style:none;
	margin-bottom:107px;
	margin-left:-12px;	
}

	ul.timeline-events li.active{
		background:#848689;
	}

		#timeline .tooltip{
			opacity:1;
			background:#e8ebef;
			color:#848689;
			display:block;
			padding:15px 0 15px 0;
			font-size:18px;
			font-family: 'Apercu Bold', Helvetica, sans-serif;
			-webkit-border-radius:8px;
			-moz-border-radius:8px;
			border-radius:8px;
			position:absolute;
		}

		#timeline .tooltip.blue{
			background:#5bbcd5;
			color:#fff;
			font-size:24px;
			padding:15px;
		}

			#timeline .tooltip:hover{
				text-decoration:none;
			}

			#timeline .tooltip.blue::after{
				top:50%!important;
			}

			#timeline .tooltip.left::after{
				content:'';
				position:absolute;
				border-top: 16px solid transparent;
				border-bottom: 16px solid transparent; 				
				border-left:12px solid #e8ebef; 
				right:-8px; top:23%;
				margin-top:-15px;		
				width:0px; height:0px;
			}				

			#timeline .tooltip.right::after{
				content:'';
				position:absolute;
				border-top: 14px solid transparent;
				border-bottom: 14px solid transparent; 				
				border-right:12px solid #e8ebef; 
				left:-8px; top:23%;
				margin-top:-15px;		
				width:0px; height:0px;
			}	

			#timeline .tooltip.blue::after{
				border-left:12px solid #5bbcd5; 
			}

			#timeline .tooltip-title{
				padding:0 10px 15px 15px;
				border-bottom:1px solid #cacdd0;
			}

				#timeline .tooltip-title p{
					font-size:24px;
					line-height:24px;
					color:#848689;
					margin:0;
					font-family: 'Apercu Bold', Helvetica, sans-serif;
				}
			
			#timeline .tooltip-content{
				padding:15px 15px 0 70px;
				position:relative;
			}

				#timeline .tooltip-content img.tooltip-icon{
					position:absolute;
					top:15px; left:15px;
				}

				#timeline .tooltip-content p{
					font-size:15px;
					line-height:20px;
					color:#848689;
					font-family: 'Apercu Bold', Helvetica, sans-serif;
				}

				#timeline .tooltip-content p strong{
					font-weight:normal;
					color:#4a4d51;
				}
		
		#t-tooltip-1{
			top:-10px;
			right:30px;
			width:175px;
		}

		#t-tooltip-2{
			width:360px;
			top:112px;
			left:40px;
		}

		#t-tooltip-3{
			top:253px;
			width:357px;
			right:30px;
		}

		#t-tooltip-4{
			top:388px;
			width:354px;
			left:40px;
		}

		#t-tooltip-5{
			top:527px;
			width:357px;
			right:30px;
		}

body .gform_wrapper .gform_footer{
	overflow:auto;
}

.button, body .gform_wrapper .gform_footer input[type=submit]{
	display:block;
	width:366px;
	height:82px;
	margin:0 auto;
	line-height:77px;
	font-size:26px;
	color:#fff;
	text-align:center;
	font-family: 'Apercu Bold', Helvetica, sans-serif;
	text-transform:uppercase;
	background:#84cde0;
	-webkit-border-radius:10px;
	-moz-border-radius:10px;
	border-radius:10px;
	border:1px solid #3fa5be;
	-webkit-box-shadow:0 2px 1px rgba(0,0,0,0.2), inset 0 0 4px rgba(255,255,255,0.9);
	-moz-box-shadow:0 2px 1px rgba(0,0,0,0.2), inset 0 0 4px rgba(255,255,255,0.9);
	box-shadow:0 2px 1px rgba(0,0,0,0.2), inset 0 0 4px rgba(255,255,255,0.9);
	background-image: linear-gradient(bottom, #60BED6 0%, #8CCFE2 100%);
	background-image: -o-linear-gradient(bottom, #60BED6 0%, #8CCFE2 100%);
	background-image: -moz-linear-gradient(bottom, #60BED6 0%, #8CCFE2 100%);
	background-image: -webkit-linear-gradient(bottom, #60BED6 0%, #8CCFE2 100%);
	background-image: -ms-linear-gradient(bottom, #60BED6 0%, #8CCFE2 100%);

	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0, #60BED6),
		color-stop(1, #8CCFE2)
	);	
}

	body .gform_wrapper .gform_footer input[type=submit]{
		float:left;
	}

	.button:hover, body .gform_wrapper .gform_footer input[type=submit]:hover{
		color:#fff;
		text-decoration:none;
		background-image: linear-gradient(bottom, #60BED6 0%, #95DEF0 100%);
		background-image: -o-linear-gradient(bottom, #60BED6 0%, #95DEF0 100%);
		background-image: -moz-linear-gradient(bottom, #60BED6 0%, #95DEF0 100%);
		background-image: -webkit-linear-gradient(bottom, #60BED6 0%, #95DEF0 100%);
		background-image: -ms-linear-gradient(bottom, #60BED6 0%, #95DEF0 100%);

		background-image: -webkit-gradient(
			linear,
			left bottom,
			left top,
			color-stop(0, #60BED6),
			color-stop(1, #95DEF0)
		);		
	}

	.button:active, body .gform_wrapper .gform_footer input[type=submit]:active{
		-webkit-box-shadow:0 2px 1px rgba(255,255,255,0.2), inset 0 2px 2px rgba(0,0,0,0.3);
		-moz-box-shadow:0 2px 1px rgba(255,255,255,0.2), inset 0 2px 2px rgba(0,0,0,0.3);
		box-shadow:0 2px 1px rgba(255,255,255,0.2), inset 0 2px 2px rgba(0,0,0,0.3);
		background-image: linear-gradient(bottom, #60BED6 100%, #95DEF0 0%);
		background-image: -o-linear-gradient(bottom, #60BED6 100%, #95DEF0 0%);
		background-image: -moz-linear-gradient(bottom, #60BED6 100%, #95DEF0 0%);
		background-image: -webkit-linear-gradient(bottom, #60BED6 100%, #95DEF0 0%);
		background-image: -ms-linear-gradient(bottom, #60BED6 100%, #95DEF0 0%);

		background-image: -webkit-gradient(
			linear,
			left bottom,
			left top,
			color-stop(1, #60BED6),
			color-stop(0, #95DEF0)
		);		
	}

.request-pricing{
	font-size:16px;
