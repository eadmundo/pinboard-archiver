@import url('fonts/fonts.css');
 
/* 
 * Styles Reset
 * ===============================
 */
 
body, html, ol, ul, dl, dd, dt, li, p, h1, h2, h3, h4, h5, h6, div, span, strong, em, table, td, tr {
	margin: 0;
	padding: 0;
}

a {
  outline: none;
	text-decoration: none;
}

.non-home .content-right a, .non-home .content-right a:active {
  color: #40A6E1;
	outline: none;
	text-decoration: none;
}

.non-home .content-right a:visited {
  color: #d16600;
}

.non-home .content-right a:hover {
  color: #FB9700;
}

ol, ul, li {
	list-style: none;
}

/* 
 * END: Styles Reset
 * ===============================
 */


/* 
 * General Styles
 * ===============================
 */

body {
	background: url('../images/bkg_grey-noise.png');
	color: #666;
	font-family: Arial, Helvetica, Geneva, sans-serif;
}

.po {
	position: relative;
	overflow: hidden;
}
.container {
	width: 960px;
	margin: 0 auto;
	position: relative;
	overflow: hidden;
}

.block {
  display:block;
}

.hidden {
  display:none;
}

.fw {
	width: 100%;
	height: auto;
}

.btn-orange {
	display: inline-block;
	position: relative;
	overflow: hidden;
	height: 59px;
	width: auto;
	font: 25px/59px 'MiloBlack', 'Arial Unicode MS';
	text-transform: uppercase;
	color: #FFF;
	text-shadow: 1px 1px 0 #b86400;

}

.btn-orange .btn-text {
	display: block;
	height: 59px;
	background: url('../images/bkg_btn-orange.png') repeat-x;
	padding: 0 10px;
	float: left;
	text-align: center;
}
dl.slidedeck .slide_2 .btn-orange .btn-text{
	width:162px;
	padding: 0 25px;
	font-size: 24px;
}

dl.slidedeck .slide_2 .btn-blue .btn-text {
	padding: 0 15px;
	font-size: 24px;
	width:260px;
}

dl.slidedeck .slide_2 .btn-orange .btn-text.ja,
dl.slidedeck .slide_2 .btn-blue .btn-text.ja {
  width: 220px;
}

.btn-orange .btn-cap {
	display: block;
	float: left;
	height: 59px;
	width: 9px;
}

.btn-orange .btn-cap.left {
	background: url('../images/cap_btn-orange-left.png') no-repeat;
}


.btn-orange .btn-cap.right {
	background: url('../images/cap_btn-orange-right.png') no-repeat;
}

.btn-orange:hover .btn-text,
.btn-orange:hover .btn-cap {
	background-position: 0 -59px !important;
}


.btn-blue {
	display: inline-block;
	position: relative;
	overflow: hidden;
	height: 59px;
	width: auto;
	font: 25px/59px 'MiloBlack', 'Arial Unicode MS';
	text-transform: uppercase;
	color: #FFF;
	text-shadow: 1px 1px 0 #1e555b;
	top: -1px;
}

.btn-blue .btn-text {
	display: block;
	height: 59px;
	background: url('../images/bkg_btn-blue.png') repeat-x;
	padding: 0 6px;
	float: left;
	text-align: center;
}

.btn-blue .btn-cap {
	display: block;
	float: left;
	height: 59px;
	width: 9px;
}

.btn-blue .btn-cap.left {
	background: url('../images/cap_btn-blue-left.png') no-repeat;
}

.btn-blue .btn-cap.right {
	background: url('../images/cap_btn-blue-right.png') no-repeat;
}

.btn-blue:hover .btn-text,
.btn-blue:hover .btn-cap {
	background-position: 0 -59px !important;
}

dl.slidedeck .btn-orange .btn-text,
dl.slidedeck .btn-blue .btn-text {
	width:240px;
}

.logo-main {
	background: url('../images/logo_ginzametrics.png') no-repeat;
	position: absolute;
	overflow: hidden;
	width: 217px;
	height: 0;
	padding-top: 36px;
	line-height: 100px;
	display: block;
	top: 70px;
	left: 0;
}

/* 
 * END: General Styles
 * ===============================
