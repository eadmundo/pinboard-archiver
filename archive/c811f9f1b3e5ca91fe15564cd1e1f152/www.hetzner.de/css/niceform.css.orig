.outtaHere {
	position:absolute;
	left:-3000px;
}


/*Selects*/
.selectArea {
	width: 107px;
	height: 20px;
	float: left; 
	position: relative;

	margin: 0;
	background-color: #FFF;
}

.selectArea .left {
	height: 20px;
	float: left;
	border-left: 1px solid #DDDDDD;
}
.selectArea .right {
	width: 17px !important;
	height: 20px;
	float: right;
	background: url(/images/select_right.gif) no-repeat left top;
}

.selectArea .right a {
	display: block;
	width: 17px;
	height: 20px;
	overflow: hidden;
	position: absolute; 
	line-height: 20px;
}
.selectArea .right i {display:none;}
.selectArea .center, div.form .selectArea .center {
	width: 81px; 
	line-height: 18px /* important */;
	height: 18px;
	padding: 0 4px 0 4px;
	overflow: hidden;
	font-weight: bold;
	float: left;
	margin: 0;
	border-top: 1px solid #DDD;
	border-bottom: 1px solid #DDD;
}

/*Selects drop-down*/
.optionsDivInvisible, .optionsDivVisible {
	position: absolute;
	top: 20px;
	left: 0;
	margin: 0;
	width: 105px;
	background-color: #FFF;
	z-index: 20;
	float: left;
	border-left: 1px solid #DDDDDD;
	border-right: 1px solid #DDDDDD;
	border-top: 1px solid #DDDDDD;
	overflow: hidden;
}
.optionsDivInvisible {display:none;}
.optionsDivVisible {display:block;}
.optionsDivVisible p {
	margin:0 !important;
	padding:0 !important;
	position: relative;

}
.optionsDivVisible a, .optionsDivVisible a {
	color: #4D4D4D;
	text-decoration: none;
 	display: block;
	width: 100%;
	padding: 1px 4px 1px 4px;
	border: none;
	line-height: 20px;
	height: 20px;
	font-weight: normal;
	margin: 0;
	border-bottom: 1px solid #D8D8D8;
}
.optionsDivVisible a:hover, .optionsDivVisible a:hover {
	background:#F7F7F7;
	text-decoration: none;
}
