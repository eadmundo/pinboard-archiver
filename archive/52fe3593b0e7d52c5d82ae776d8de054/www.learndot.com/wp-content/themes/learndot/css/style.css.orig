/* talking-posts */

#talking-posts{
	padding:60px 0 80px;
}

	#talking-posts .span12 > p{
		font-size:24px;
		line-height:28px;
		color:#6d6f72;
		margin-bottom:40px;
		padding-right:180px;
	}

	#talking-posts .span12 > p a{
		color:#69c2d9;
		text-decoration:underline;
	}

		#talking-posts .span12 > p a:hover{
			color:#2189A4;
		}

	.article-list{
		margin:0; padding:0;
	}

		.article-list li{
			list-style:none;
			border-bottom:1px solid #dedede;
		}

		.article-list li.last, .article-list li:last-child {
			border-bottom:none;
		}

		.article-list li a{
			display:block;
			padding:5px 80px 30px 0;
			position:relative;
		}

			.article-list li a:hover{
				text-decoration:none;
			}

			.article-list li a img{
				position:absolute;
				top:44px; left:0px;
				-webkit-border-radius:90px;
