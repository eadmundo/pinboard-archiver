 */

.mini-header {
	height: 142px !important;
	background: url('../images/bkg_dark-squares.png') repeat-x;
	position: relative !important;
	border-bottom: 1px solid #FFF;
}

.bcont {
	background: #FCFCFC;
	padding-left: 20px;
	width: 940px;
	box-shadow: 0 0 5px rgba(0,0,0,0.33);
	border-bottom-left-radius: 6px;
	border-bottom-right-radius: 6px;
}

.bcont h2 {
	color: #121212;
	font: 24px 'MiloBold', 'Arial Unicode MS';
	padding: 0 0 10px 0;
}

#blog-sub-head-list {
	position: relative;
	overflow: hidden;
	padding-top: 20px;
	margin-bottom: 27px;
}

#blog-sub-head-list li {
	position: relative;
	overflow: hidden;
	float: left;
	font: bold 12px Helvetica, Arial, Geneva, sans-serif;
	color: #363636;
	margin-left: 30px;
}

#blog-sub-head-list li:first-child {
	margin-left: 0;
}

.recent-posts a,
#blog-sub-head-list a {
	color: #363636;
	display: block;
}

.recent-posts a:hover,
#blog-sub-head-list a:hover {
	color: #40A6E1;
}

#blog-sub-head-list img {
	display: block;
	padding: 3px;
	border: 1px solid #D2D2D2;
	background: #FFF;
	margin-bottom: 7px;
	width: 168px;
	height: 115px;
}

.recent-posts a:hover img,
#blog-sub-head-list a:hover img {
	background: #E6E6E6;
}


#get-email-updates,
#blog-search {
	position: relative;
}

#get-email-updates {
	background: url('../images/bkg_get-email-updates.png') 0 0 no-repeat;
	height: 101px;
	width: 273px;
	padding: 30px 24px 25px;
	margin-bottom: 30px;
}

#get-email-header {
	display: block;
	font: bold 18px Helvetica, Arial, Geneva, sans-serif;
	color: #FFF;
	margin-bottom: 3px;
}

#get-email-updates form {
	display: block;
	position: relative;
	overflow: hidden;
}

/* To override MailChimp widget values */
/* look for other mc_* attributes for MailChimp values */
#mc_signup,
.mc_signup_form {
  width: 290px;
  margin-left: -5px;
}

.mc_merge_var {
  width: 165px;
}

.mc_var_label {
  display: none;
}

#mc_mv_EMAIL.mc_input,
.comment-area textarea,
#get-email-updates input[type=text],
#blog-search input[type=text] {
	border-radius: 3px;
	background: #FFF;
	border: none;
	height: 24px;
	width: 165px;
	float: left;
	margin-right: 7px;
	box-shadow: inset 0 2px 1px #CCC;
	color: #DADADA;
	font: bold 14px Helvetica, Arial, Geneva, sans-serif;
	padding: 2px 11px 0;
	position: relative;
	top: 1px;
	line-height: 24px;
}

#get-email-updates .input-wrapper,
#blog-search .input-wrapper,
.mc_signup_submit {
	position: relative;
	overflow: hidden;
	float: left;
	height: 29px;
}

#get-email-updates .input-wrapper:hover input[type="image"],
#blog-search .input-wrapper:hover input[type="image"],
#mc_signup_submit:hover {
	position: relative;
	top: -29px;
}

#get-email-updates p {
	margin: 0;
	padding: 0;
	position: relative;
	overflow: hidden;
	top: 10px;
	font: normal 12px/15px Helvetica, Arial, Geneva, sans-serif;
	color: #FFF;
}

.category-list {
	position: relative;
	overflow: hidden;
	text-align: center;
	font: 12px/16px 'MiloMedium', 'Arial Unicode MS';
	color: #252525;
	float: left;
	width: 100%;
	padding: 11px 0 10px;
	border-top: 1px solid #e6e6e6;
}

.category-list li {
	float: left;
	margin-left: 9px;
  width: 176px;
}

.category-list li:first-child {
	text-transform: uppercase;
	margin-right: 5px;
}

.category-list a {
	color: #252525;
	padding: 0 12px;
	display: block;
	width: auto;
	float: left;
	height: 22px;
	position: relative;
}

.category-list a:hover {
	color: #40A6E1;
}

.category-list a.active {
	background: url('../images/bkg_blog-topic-active.png') 0 0 repeat-x;
	color: #FFF;
	cursor: default;
	text-shadow: 1px 1px 0 rgba(17,96,139,0.45);
}

.category-list a.active .cat-active-cap {
	display: block;
	height: 22px;
	position: absolute;
	top: 0;
}

.category-list a.active .cat-active-cap.left {
	left: 0;
	background: url('../images/bkg_cat-active-cap-left.png') 0 0 no-repeat;
	width: 7px;
}
.category-list a.active .cat-active-cap.right {
	right: -2px;
	background: url('../images/bkg_cat-active-cap-right.png') 0 0 no-repeat;
	width: 8px;
}

.blog-content {
	float: left;
	width: 588px;
}

.blog-sidebar {
	float: right;
	width: 321px;
	background: url('../images/pattern_blue-lines.gif');
	height: 100%;
	padding-bottom: 60px;
}

#blog-search {
	height: 43px;
	background: url('../images/bkg_blue-header-boxes.png') center center no-repeat;
	width: 290px;
	margin: 0 auto;
	border-radius: 8px;
}

#blog-search form {
	width: 260px;
	margin: 0 auto;
	position: relative;
	overflow: hidden;
	top: 7px;
}

#blog-search input[type="text"] {
	width: 165px;
	background: #FFF url('../images/icon_mag-glass.png') 0 -1px no-repeat;
	padding-left: 26px;
}

.recent-posts {
	color: #363636;
	font-size: 16px;
	padding: 0 5px 0 23px;
}

.blog-sidebar h2 {
	padding: 15px 0 0 22px;
}

.recent-posts li {
	padding-top: 30px;
	position: relative;
	overflow: hidden;
}

.recent-posts li:first-child {
	padding-top: 20px;
}

.recent-posts img {
	float: left;
	padding: 3px;
	border: 1px solid #D2D2D2;
	background: #FFF;
	margin-right: 14px;
}

.recent-posts .recent-title {
	display: block;
	margin-bottom: 5px;
	font-weight: bold;
	padding-top: 8px;
	font-size: 14px;
}

.recent-posts .recent-date {
	color: #a1a1a1;
	font-size: 13px;
}

.post-body {
	position: relative;
	overflow: hidden;
	clear: both;
	/*border-top: 1px solid #e6e6e6;*/
	padding-top: 20px;
	margin-bottom: 10px;
}

.post-img-col {
	width: 140px;
	position: relative;
	overflow: hidden;
	clear: both;
	float: left;
	margin-right: 24px;
	padding-top: 5px;
}

.post-img-col img {
	padding: 3px;
	border: 1px solid #d2d2d2;
}


.blog .post-social {
  float: left;
margin: 15px 0;
  width: 75%;
}

.blog.single .post-social {
  
  float: left;
}

.blog .post-social .fb_iframe_widget {
  top: -4px;
}

.blog.single .post-social .fb_iframe_widget {
  float: left;
  top: 0;
}

.post-gplus {
	float: left;
}

.post-content {
	width: 424px;
	float: left;
}

.blog.single .post-content {
	/*width: auto;*/
  width: 588px;
}

.post-header {
	padding-bottom: 25px;
	margin-bottom: 25px;
}
.clear{clear:both;}

.post-avatar{float:left;margin:0 20px 0 0;}

.post-title {
	font: 16px 'MiloBold', 'Arial Unicode MS';
	color: #40a6e1;
	margin-bottom: 0px;
}

.blog.single .post-header {
	border: none;
  margin-top: 20px;
}

.post-content h3,
.post-content h3 a {
	font: 24px 'MiloBold', 'Arial Unicode MS';
	color: #40a6e1;
	margin-bottom: 5px;
}

.post-content h3 a:hover {
  color: #338CC0;
}

.blog.single .post-content h3 {
	font-size: 18px;
	margin-bottom: 12px;
}

.post-meta {
	font-size: 13px;
	color: #666;
}

.blog.single .post-meta {
	margin:5px 0;
	float: left;
}

.post-content p {
	font: 14px/20px Helvetica, Arial, Geneva, sans-serif;
	color: #252525;
	margin-bottom: 15px;
}

.post-content ol,
.post-content ul {
  margin: 12px 0;
  font: 14px/18px Helvetica,Arial,Geneva,sans-serif
}

.page ul,
.post-content ul,
.page ol,
.post-content ol {
  margin: 10px 0 20px;
}

.page ul li,
.post-content ul li {
  list-style: disc;
  margin: 9px 0 0 30px
}

.page ol li,
.post-content ol li {
  list-style: decimal;
  margin: 9px 0 0 30px
}

.read_more {
  float: right;
}

.read-more-link {
	color: #fb9700;
	text-decoration: underline;
	/*background: url('../images/icon_arrow-orange.png') right 5px no-repeat;*/
	padding-right: 14px;
}

.read-more-link:hover {
	color: #252525;
	text-decoration: none;
	background-position: right -7px;
}

.blog.single .fb-like,
.post-footer .fb-like {
	width: 53px;
	position: relative;
	overflow: hidden;
	float: left;
}

.blog.single .twitter-share-button {
	float: left;
}

.blog.single .fb-like,
.blog.single .post-gplus {
	float: left;
	margin-left: 0;
}

.post-comment-count {
	background: url('../images/bkg_comment-count.png') center top no-repeat;
	height: 32px;
	position: relative;
	width: auto;
	float: left;
	font-size: 13px;
	line-height: 24px;
	color: #FFF !important;
	padding: 0 8px;
	margin-left: 15px;
}

.comment-cap {
	position: absolute;
	height: 24px;
	width: 4px;
	display: block;
	top: 0;
}

.comment-cap.left {
	left: 0;
	background: url('../images/bkg_comment-count-cap-left.png') 0 0 no-repeat;
}
.comment-cap.right {
	right: 0;
	background: url('../images/bkg_comment-count-cap-right.png') 0 0 no-repeat;
}

.blog.single .post-content img {
	padding: 3px;
	background: #FFF;
	border: 1px solid #d2d2d2;
}

.blog.single .post-image {
	margin-bottom: 15px;
}

.comment-area textarea {
	width: 557px;
	height: 130px;
	padding: 20px 15px;
	resize: none;
	border-bottom: 1px solid #e4e4e4;
	margin-bottom: 20px;
}

.addnewcomm {
	font-size: 12px;
	color: #a1a1a1;
	display: block;
	margin-bottom: 20px;
	border-top: 1px solid #E6E6E6;
	padding-top: 20px;
}

.comment-submit-wrapper {
	position: relative;
	overflow: hidden;
	float: right;
	height: 29px;
	width: 62px;
}

.comment-submit-wrapper input {
	position: relative;
}

.comment-submit-wrapper:hover input {
	top: -29px;
}

.textAdded {
	color: #252525 !important;
}

#nav-single {
 margin: 5px 0 45px 0;
}


